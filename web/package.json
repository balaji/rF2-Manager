{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@types/node": "^10.7.0",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.7",
    "inline-assets": "^1.4.6",
    "parcel-bundler": "1.10.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "parcel": "$(npm bin)/parcel build -d ../dist --public-url './' ./src/index.html -o output.html",
    "inline": "$(npm bin)/inline-assets ../dist/output.html ../dist/index.html",
    "build": "yarn parcel && yarn inline",
    "clean-build": "yarn clean && yarn build",
    "dev": "$(npm bin)/tsc --noEmit --watch & $(npm bin)/parcel ./src/index.html"
  }
}
